# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git - it contains sensitive information

# Twelve Data API Key (get from https://twelvedata.com/)
TWELVE_DATA_API_KEY=your_twelve_data_api_key_here

# Telegram Bot Configuration (get from @BotFather on Telegram)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Optional: Port for web server (Render will set this automatically)
PORT=5000
